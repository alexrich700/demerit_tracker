<!--TEST-->

<div class="container">
    <section>
        <div class="row">
            
                <div class="col-md-2"> Employees Name</div>
                    <span class="col-md-1">|</span>
                <div class="col-md-2"> Number Of Tally Marks Received </div>
                    <span class="col-md-1">|</span>
                <div class="col-md-2"> Number Of Strikes Received </div>
                    <span class="col-md-1">|</span>
                <div class="col-md-3" id="demo"> Total Number Of Marks </div>
                <hr>
        </div>
        <div class="row">
            <% employee.forEach(function(employee){ %>
            
                <% var x = employee.tallymark; %>
                <% var y = employee.strike; %>
                <% var f = y * 3; %>
                <% var total = f + x; %>
            
                      <div class="col-md-2"> <%= employee.name %> </div>
                          <span class="col-md-1">|</span>
                      <div class="col-md-2"> <%= employee.tallymark %> </div>
                          <span class="col-md-1">|</span>
                      <div class="col-md-2"> <%= employee.strike %> </div>
                          <span class="col-md-1">|</span>
                      <div class="col-md-3" id="demo">
                          <%= total %>
                      </div>
                      <hr>
                <% }) %>
            </div>
        </div>
  </section> 
</div>
<% include partials/footer%>